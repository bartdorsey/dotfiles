{
    "$schema": "https://github.com/glzr-io/zebar/raw/v3.1.0/resources/zpack-schema.json",
    "name": "gruvbox-zebar",
    "version": "0.0.0",
    "description": "",
    "tags": [],
    "previewImages": [],
    "repositoryUrl": "",
    "widgets": [
        {
            "name": "gruvbox",
            "htmlPath": "./dist/index.html",
            "zOrder": "normal",
            "shownInTaskbar": false,
            "focused": false,
            "resizable": false,
            "transparent": true,
            "includeFiles": ["**/**"],
            "caching": {
                "defaultDuration": 604800,
                "rules": []
            },
            "privileges": {
                "shellCommands": []
            },
            "presets": [
                {
                    "name": "default",
                    "anchor": "top_left",
                    "offsetX": "0px",
                    "offsetY": "0px",
                    "width": "100%",
                    "height": "40px",
                    "monitorSelection": {
                        "type": "all"
                    },
                    "dockToEdge": {
                        "enabled": true,
                        "edge": null,
                        "windowMargin": "0px"
                    }
                }
            ]
        }
    ]
}
