{
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "python.analysis.typeCheckingMode": "standard",
    "vim.leader": "<space>",
    "vim.highlightedyank.enable": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "/"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "s"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "television.ToggleFileFinder"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "r"
            ],
            "commands": [
                "television.ToggleTextFinder"
            ]
        },
        {
            "before": [
                "<leader>",
                "<space>"
            ],
            "commands": [
                "workbench.action.showAllEditors"
            ]
        }
    ],
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[django-html]": {
        "editor.defaultFormatter": "monosans.djlint"
    },
    "explorer.compactFolders": false,
    "workbench.tree.indent": 20,
    "emmet.includeLanguages": {
        "django-html": "html",
        "jinja-html": "html",
        "javascript": "javascriptreact"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "redhat.telemetry.enabled": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "markdown-mermaid.lightModeTheme": "dark",
    "mermaid-editor.preview.backgroundColor": "black",
    "vim.useSystemClipboard": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "window.zoomLevel": 1,
    "remote.SSH.remotePlatform": {
        "*.gitpod.io": "linux",
        "nixos-dev": "linux",
        "python-test.devpod": "linux"
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "git.openRepositoryInParentFolders": "always",
    "workbench.startupEditor": "none",
    "database-client.highlightSQLBlock": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "totalTypeScript.hideAllTips": true,
    "totalTypeScript.hideBasicTips": false,
    "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
    "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
    "editor.minimap.enabled": false,
    "cmake.showOptionsMovedNotification": false,
    "python.analysis.autoFormatStrings": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.analysis.inlayHints.pytestParameters": true,
    "workbench.iconTheme": "catppuccin-mocha",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.renderSideBySide": false,
    "editor.accessibilitySupport": "off",
    "vim.sneak": true,
    "vim.sneakReplacesF": true,
    "open-in-vim.useNeovim": true,
    "djlint.showInstallError": false,
    "security.promptForRemoteFileProtocolHandling": false,
    "lldb.suppressUpdateNotifications": true,
    "database-client.autoSync": true,
    "remote.SSH.useExecServer": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "workbench.colorTheme": "Catppuccin Mocha",
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "markdown-preview-enhanced.mermaidTheme": "dark",
    "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
    "plpgsqlLanguageServer.database": "book_store",
    "plpgsqlLanguageServer.password": "postgres",
    "plpgsqlLanguageServer.user": "postgres",
    "css.styleSheets": [
        "**/*.css"
    ],
    "html.format.indentInnerHtml": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "basedpyright.analysis.diagnosticMode": "workspace",
    "basedpyright.analysis.typeCheckingMode": "standard",
    "editor.lineHeight": 0,
    "editor.fontSize": 15,
    "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
    "editor.codeLensFontSize": 14,
    "editor.inlayHints.fontSize": 14,
    "editor.codeLensFontFamily": "Iosevka",
    "editor.inlayHints.fontFamily": "Iosevka",
    "errorLens.problemRangeDecorationEnabled": true,
    "errorLens.fontSize": "-3px",
    "errorLens.gutterIconSet": "square",
    "errorLens.fontFamily": "Iosevka",
    "errorLens.followCursor": "closestProblem",
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "all",
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "python.analysis.generateWithTypeAnnotation": true,
    "python.analysis.typeEvaluation.strictDictionaryInference": true,
    "python.analysis.typeEvaluation.strictListInference": true,
    "python.analysis.typeEvaluation.strictSetInference": true,
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "svelte.enable-ts-plugin": true,
    "vim.enableNeovim": true,
    "[nix]": {
        "editor.defaultFormatter": "jnoortheen.nix-ide"
    },
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "latex-workshop.view.pdf.invert": 1,
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "css": false
    },
    "editor.rulers": [
        80
    ],
    "editor.inlayHints.enabled": "off",
    "workbench.editor.empty.hint": "hidden",
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingStrategy": "advanced",
    "tinymist.lint.enabled": true
}